Xcursor.theme: 		volantes_cursors

Rxvt.background:	rgb:38/38/40
URxvt.foreground:	rgb:c8/c8/c8
URxvt.font:		xft:Source Code Pro:pixelsize=12,PowerlineSymbols:pixelsize=12
URxvt.letterSpace:	1
URxvt.geometry:		100x36
URxvt.scrollstyle:	plain
URxvt.scrollBar:	False
URxvt.scrollBar_right:	True
URxvt.cursorBlink:	True
URxvt.url-launcher:	firefox
URxvt.saveLines:	8192

URxvt.transparent:	True
URxvt.shading:		20

URxvt.perl-ext-common:	default

Xcursor.size:		16

XTerm.termName:		xterm-256color
XTerm.vt100.locale: 	true
XTerm.ttyModes: 	erase ^?

XTerm.vt100.renderFont: true

XTerm.vt100.saveLines: 	4096

XTerm.vt100.metaSendsEscape: 	false
XTerm.vt100.backarrowKey: 	false
XTerm.vt100.reverseVideo: 	true

XTerm.vt100.selectToClipboard:	true

