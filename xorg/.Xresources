XTerm*faceName: Source Code Pro:pixelsize=12:antialias=true:autohint=true,PowerlineSymbols:pixelsize=12:antialias=true:autohint=true,Font Awesome 5 Free:pixelsize=12:antialias=true:style=solid:autohint=true
!XTerm.vt100.faceName: Source Code Pro:size=10:antialias=true:autohint=true
!XTerm.vt100.faceName: DejaVu Sans Mono for Powerline:size=10:antialias=true
XTerm.scaleHeight:	1.0

XTerm.vt100.translations: #override 			\n\
	Ctrl Alt <Key>S: 	secure()		\n\
	Shift <KeyPress> Home: 	scroll-to(begin) 	\n\
	Shift <KeyPress> End:	scroll-to(end)		\n\
	Ctrl Shift <Key> C:	copy-selection(SELECT)	\n\
	Ctrl Shift <Key> V:	insert-selection(SELECT)\n\
	Ctrl <Key> +:		larger-vt-font()	\n\
	Ctrl <Key> -:		smaller-vt-font()	\n\
	Ctrl <Key> 0:		set-vt-font(d)		\n\
	Ctrl Shift <Key> T:	spawn-new-terminal()

xscreensaver.splash: false
xscreensaver.captureStderr: false
xscreensaver.timeout: 0

